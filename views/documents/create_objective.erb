<div class='row'>
    <h2>Create Document > Objective</h2>
</div>


<!-- List of projects/docs -->
<div class='row'>
    <div class='col-md-6'>
        <form role="form">
            <div class="form-group">
                <label for="projectGoals">What is your primary objective for this project?</label>
                <textarea type="text" id="projectGoals" class="form-control" rows="2"></textarea>
            </div>
            <div class="form-group">
                <label for="responsible">What are your other objectives for this project?</label>
                <textarea type="text" id="responsible" class="form-control" rows="2"></textarea>
            </div>
            <div class="form-group">
                <label for="responsible2">What are your other goal/objectives for this project?</label>
                <textarea type="text" id="responsible2" class="form-control" rows="2"></textarea>
            </div>
            <div class="form-group col-md-offset-9 col-md-4" >
                    <a class="btn btn-large btn-info" href="/projects/1/doctype/1">Previous</a>
                    <a class="btn btn-large btn-info" href="/projects/<%=project_id%>/doctype/<%=doctypeid%>/create_goals">Next</a>
            </div>
        </form>
    </div>
</div>