<div class='row'>
    <h2>Create Document > Objective</h2>
</div>


<!-- List of projects/docs -->
<div class='row'>
    <div class="col-md-8">
        <form role="form">
            <div class="form-group">
                <label for="projectGoals">What is your primary objective for this project?</label>
                <textarea type="text" id="projectGoals" class="form-control" rows="3"></textarea>
            </div>
            <div class="form-group">
                <label for="goals">What are your other objectives for this project?</label>
                <textarea type="text" id="goals" class="form-control" rows="3"></textarea>
            </div>

            <div class="form-group">
                <label for="goals2">What are your other goal/objectives for this project?</label>
                <textarea type="text" id="goals2" class="form-control" rows="3"></textarea>
            </div>

            <div class="form-group col-md-offset-9 col-md-4" >
                <a class="btn btn-large btn-info" href="/projects/<%=project_id%>/doctype/<%=doctypeid%>/create_objective">Previous</a>
                <a class="btn btn-large btn-info" href="/projects/<%=project_id%>/doctype/<%=doctypeid%>/create_risks">Next</a>
            </div>
        </form>
    </div>
</div>